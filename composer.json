{
    "name": "Loren138/cas-server",
    "description": "Laravel IDE Helper, generates correct PHPDocs for all Facade classes, to improve auto-completion.",
    "keywords": ["laravel", "autocomplete", "ide", "helper", "phpstorm", "netbeans", "sublime", "codeintel", "phpdoc"],
    "license": "MIT",
    "authors": [
        {
            "name": "Loren Klingman",
            "homepage": "http://klingman.us",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "laravel/framework": "5.1.x|5.2.x|5.3.x",
        "laravelcollective/html": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.*",
        "orchestra/testbench": "3.2.x",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Loren138\\CASServer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Loren138\\CASServerTests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs": "phpcs --standard=psr2 src/"
    }
}
